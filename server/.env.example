# Server Configuration
NODE_ENV=development
PORT=3001
BASE_URL=http://localhost:3001

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://tryquotely.com

# API Keys - TurboRater
TURBORRATER_API_URL=https://api.turborrater.com/v1
TURBORRATER_API_KEY=your_turborrater_api_key
TURBORRATER_ACCOUNT_ID=your_account_id
TURBORRATER_AGENT_ID=your_agent_id

# API Keys - Momentum
MOMENTUM_API_URL=https://api.momentum.com/v2
MOMENTUM_API_KEY=your_momentum_api_key
MOMENTUM_CLIENT_ID=your_client_id

# API Keys - GAIL
GAIL_API_URL=https://api.gail-insurance.com/v3
GAIL_API_KEY=your_gail_api_key
GAIL_PARTNER_ID=your_partner_id

# Redis Cache (optional)
REDIS_URL=redis://localhost:6379
CACHE_TTL=300

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090