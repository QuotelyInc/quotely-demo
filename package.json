{
  "name": "tryquotely",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "check-branch": "git branch --show-current | grep -q '^master$' || (echo 'Error: Not on master branch!' && exit 1)",
    "sitemap": "node scripts/automated-sitemap-deploy.js",
    "sitemap:generate": "node scripts/automated-sitemap-deploy.js --generate-only",
    "sitemap:validate": "node scripts/automated-sitemap-deploy.js --validate-only",
    "sitemap:deploy": "npm run check-branch && node scripts/automated-sitemap-deploy.js"
  },
  "dependencies": {
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
