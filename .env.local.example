# Environment Variables Configuration
# Copy this file to .env.local and add your actual API keys
# NEVER commit .env.local to version control

# AI Services
CLAUDE_API_KEY=your_claude_api_key_here
GROK_API_KEY=your_grok_api_key_here
OPENAI_API_KEY=your_chatgpt_pro_key_here

# Insurance Quote APIs
TURBO_RATER_API_KEY=your_turbo_rater_key_here
GAIL_API_KEY=your_gail_api_key_here
MOMENTUM_API_KEY=your_momentum_api_key_here

# Next.js Public Variables (accessible in browser)
NEXT_PUBLIC_OTTO_UUID=93fecead-4a44-4a94-8620-c45564441a5b
NEXT_PUBLIC_OTTO_ENABLED=true
NEXT_PUBLIC_SITE_URL=https://tryquotely.com

