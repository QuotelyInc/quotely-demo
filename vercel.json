{
  "public": true,
  "github": {
    "enabled": true
  },
  "git": {
    "deploymentEnabled": {
      "master": true
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}